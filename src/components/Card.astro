---
interface Props {
  class?: string;
  [key: string]: unknown;
}

const { class: className, ...props } = Astro.props;
---

<div
  class:list={[
    "rounded-xl border relative dark:bg-neutral-500/10 dark:border-neutral-400/10 border-neutral-700/10 bg-neutral-400/10 ",
    className,
  ]}
  {...props}
>
  <slot />
</div>
