---
import { resume } from "@/data/resume";
import Section from "../Section.astro";
import StackCard from "../StackCard.astro";

const stack = resume.stack;
---

<Section title="Habilidades">
  <div class="flex flex-wrap gap-3 print:gap-5 mt-2">
    {stack.map((tag) => <StackCard name={tag.name} icon={tag.icon} />)}
  </div>
</Section>
